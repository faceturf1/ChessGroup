+package prog;
+
+public class turns {
+    Player player1 = new Player();
+    Player player2 = new Player();
+
+    Player currentPlayer = new Player();
+
+
+
+    private void takeTurns() {
+        boolean exit = false;
+        boolean isPlayer1 = true;
+
+        do {
+            if (isPlayer1) {
+                isPlayer1 = false;
+                currentPlayer = player1;
+            }
+            else {
+                isPlayer1 = true;
+                currentPlayer = player2;
+            }
+        } while (exit = false);
+    }
